$TTL    604800
@       IN      SOA     rainbow-ralph.{{ dns_zone_name }}. admin.{{ dns_zone_name }}. (
                              3         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;

; name servers - NS records
    IN      NS      rainbow-ralph.{{ dns_zone_name }}.
    IN      NS      silver-boi.{{ dns_zone_name }}.

; name servers - A records
rainbow-ralph.{{ dns_zone_name }}.       IN      A       {{ rainbow_ralph }}
silver-boi.{{ dns_zone_name }}.          IN      A       {{ silver_boi }}

; homelab - A records
black-phillip.{{ dns_zone_name }}.     IN      A      {{ black_phillip }} 
nicks-book.{{ dns_zone_name }}.        IN      A      {{ macbook }}
